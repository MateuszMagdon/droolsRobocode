package iwium.rules

import iwium.*;
import robocode.*;

import iwium.MyRobot;

query "actionQuery"
    action: Action()
end

rule "Robot has been hit"
    when
        HitByBulletEvent();
    then
        System.err.println("has been hit!");

end

rule "Two"
    when
        robot : MyRobot();
    then
        robot.ahead(1);
        System.err.println("go");
end
